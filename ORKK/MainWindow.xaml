<Window x:Class="ORKK.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ORKK"
        mc:Ignorable="d"
        Title="Over de Rhein Kraan Keuringen " Height="450" Width="800" Topmost="True" Icon="craneIcon.ico">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FFFBFFFF" Offset="1"/>
                <GradientStop Color="#FFA8F7FF"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Image x:Name="logo" Source="images/logo.png" HorizontalAlignment="Left" Height="74" VerticalAlignment="Top" Width="74" Margin="0,-7,0,0"/>

    </Grid>
</Window>
